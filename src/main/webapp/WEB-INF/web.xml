<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
          http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
    version="3.0">
    
    <servlet>
    	<servlet-name>wechatClient</servlet-name>
    	<servlet-class>com.wechat.servlet.WeChatClientServlet</servlet-class>
    </servlet>
	
	<servlet-mapping>
		<servlet-name>wechatClient</servlet-name>
		<url-pattern>/wechatClient</url-pattern>
	</servlet-mapping>
	
	<servlet>
    	<servlet-name>wechatMenu</servlet-name>
    	<servlet-class>com.wechat.servlet.WeChatMenuServlet</servlet-class>
    </servlet>
	
	<servlet-mapping>
		<servlet-name>wechatMenu</servlet-name>
		<url-pattern>/wechatMenu</url-pattern>
	</servlet-mapping>
	
	
</web-app>